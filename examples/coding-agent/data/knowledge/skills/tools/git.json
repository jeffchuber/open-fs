{
  "name": "git",
  "description": "Git version control operations",
  "commands": [
    {
      "name": "status",
      "command": "git status",
      "description": "Show working tree status"
    },
    {
      "name": "changes",
      "command": "git show --stat",
      "description": "Show recent commit changes"
    },
    {
      "name": "staged_changes",
      "command": "git status --short",
      "description": "Show staged and unstaged file states"
    },
    {
      "name": "log",
      "command": "git log --oneline -20",
      "description": "Show recent commit history"
    },
    {
      "name": "add",
      "command": "git add {files}",
      "description": "Stage files for commit",
      "parameters": {
        "files": "Files to stage (space-separated or . for all)"
      }
    },
    {
      "name": "commit",
      "command": "git commit -m \"{message}\"",
      "description": "Create a commit with message",
      "parameters": {
        "message": "Commit message"
      }
    },
    {
      "name": "push",
      "command": "git push",
      "description": "Push commits to remote"
    },
    {
      "name": "pull",
      "command": "git pull",
      "description": "Pull changes from remote"
    },
    {
      "name": "heads",
      "command": "git show-ref --heads",
      "description": "List local head refs"
    },
    {
      "name": "switch",
      "command": "git switch {topic}",
      "description": "Switch to topic",
      "parameters": {
        "topic": "Target topic name"
      }
    },
    {
      "name": "stash",
      "command": "git stash",
      "description": "Stash current changes"
    },
    {
      "name": "stash_pop",
      "command": "git stash pop",
      "description": "Apply and remove latest stash"
    }
  ]
}
