{
  "name": "ax-vfs",
  "version": "0.1.0",
  "description": "AX Virtual Filesystem - Node.js bindings",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "ax-vfs",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "vfs",
    "virtual-filesystem",
    "s3",
    "storage",
    "ai",
    "agents",
    "napi",
    "rust"
  ],
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ax"
  },
  "engines": {
    "node": ">= 16"
  }
}
